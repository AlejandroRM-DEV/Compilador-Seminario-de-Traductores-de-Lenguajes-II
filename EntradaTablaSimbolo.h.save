#ifndef ENTRATABLASIMBOLO_H_INCLUDED
#define ENTRATABLASIMBOLO_H_INCLUDED

#include <iostream>

using namespace std;

class EntradaTablaSimbolo {
private:
    static int cuentaID;
public:
    int id;
    string nombre;
    EntradaTablaSimbolo( string nombre );
    ~EntradaTablaSimbolo();


bool operator==( const EntradaTablaSimbolo& rhs ) {
    return !( nombre < rhs.nombre ) && !( nombre > rhs.nombre );
}
bool operator!=(  const EntradaTablaSimbolo& rhs ) {
    return !operator==(rhs );
}
bool operator< (  const EntradaTablaSimbolo& rhs ) {
    return nombre < rhs.nombre;
}
bool operator> ( const EntradaTablaSimbolo& rhs ) {
    return  nombre  rhs.nombre;
}
bool operator<=( const EntradaTablaSimbolo& rhs ) {
    return !operator> ( rhs );
}
bool operator>=( const EntradaTablaSimbolo& rhs ) {
    return !operator< ( rhs );
}
};


#endif // ENTRATABLASIMBOLO_H_INCLUDED
